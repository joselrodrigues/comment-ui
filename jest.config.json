{
	"preset": "ts-jest",
	"testEnvironment": "jsdom",
	"moduleNameMapper": {
		"\\.(css|less|scss|sass)$": "identity-obj-proxy",
		"\\.svg$": "<rootDir>/__mocks__/svg.ts"
	},
	"setupFilesAfterEnv": ["<rootDir>/setupTests.ts"],
	"coverageThreshold": {
		"global": {
			"branches": 80,
			"functions": 80,
			"lines": 80,
			"statements": 80
		}
	}
}

